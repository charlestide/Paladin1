{
    "name": "charlestide/paladin",
    "description": "Paladin is a Open Source Laravel Admin Panel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tangkeyu",
            "email": "tangkeyu@vip.163.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Charlestide\\Paladin\\PaladinServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=7.1",
        "laravel/framework": "5.5.*",
        "yajra/laravel-datatables-oracle": "^8.0",
        "matriphe/larinfo": "^1.0",
        "trntv/probe": "^1.0",
        "minime/annotations": "~3.0"

    },
    "autoload": {
        "psr-4": {
            "Charlestide\\Paladin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Charlestide\\Paladin\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "barryvdh/laravel-debugbar": "^3.1"
    }
}
